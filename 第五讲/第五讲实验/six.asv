%问题一代码
clc
%将明文数字编码为矩阵M
M = [77 108 100 106 100;
    114 108 101 115 101;
    32 32 32 32 32;
    72 109 116 99 32;
    106 97 104 111 32]
%%第一组密钥矩阵K1
K1 = randi([0 50],size(M))  
inv(K1)
disp('第一次加密后密文为：')
C = K1*M  %加密密文
disp('第一次解密后明文为：')
M_ = K1\C  %解密密文

%%第二组密钥矩阵K1
disp('第二组密钥矩阵：')
K2 = randi([0 50],size(M)) 
disp('第二组密钥矩阵：')
inv(K2)
disp('第二次加密后密文为：')
C = K2*M  %加密密文
disp('第二次解密后明文为：')
M_ = K2\C  %解密密文